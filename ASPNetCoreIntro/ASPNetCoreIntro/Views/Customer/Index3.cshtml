@model CustomerListViewModel
@{
    Layout = null;
}

<h1>Index3</h1>
<table>
    @foreach (var customer in Model.Customers)
    {
        <tr>
            <td>@customer.Id</td>
            <td>@customer.FirstName</td>
            <td>@customer.LastName</td>
            <td>@customer.City</td>
        </tr>

    }
    @foreach (var shop in Model.Shops)
    {
        <tr>
            <td>@shop</td>
        </tr>
    }
    </table>

